{
    "name": "gist-more-url",
    "version": "0.0.4",
    "manifest_version": 2,
    "description": "Show extend url to clone (by 'ssh' and 'git') on Gist.",
    "content_scripts": [
        {
          "matches": ["https://gist.github.com/*"],
          "js": ["vendor/js/jquery-1.9.1.min.js", "js/script.js"],
          "run_at": "document_idle"
        }
    ]
}
